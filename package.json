{
  "name": "mashlib",
  "version": "1.1.4",
  "description": "Data mashup library",
  "main": "./index.js",
  "files": [
    "index.js",
    "dist"
  ],
  "scripts": {
    "build": "npm run build:dev && npm run build:prod",
    "build:dev": "webpack --progress",
    "build:dev:local": "webpack --config-name local-webpack.config.js --progress",
    "build:prod": "webpack --progress --mode=production; echo 'Copying databrowser.html to index.html for backwards compatibility; can be removed when Node Solid Server is updated.'; cp dist/databrowser.html dist/index.html",
    "build-no-min": "npm run build:dev",
    "prepublishOnly": "npm run build",
    "standard": "standard *.js",
    "start": "webpack-dev-server --https",
    "test": "echo 'No unit tests'",
    "watch": "webpack --watch",
    "format": "prettier-standard 'src/*.ts' 'src/**/*.ts'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/solid/mashlib"
  },
  "keywords": [
    "solid",
    "decentralized",
    "widgets",
    "ui",
    "web",
    "rdf",
    "ldp",
    "panes",
    "linked",
    "data",
    "browser"
  ],
  "author": "Tim Berners-Lee <timbl@mit.edu>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/solid/mashlib/issues"
  },
  "homepage": "https://github.com/solid/mashlib",
  "dependencies": {
    "normalize.css": "^8.0.1",
    "rdflib": "^1.0.4",
    "solid-namespace": "^0.2.0",
    "solid-panes": "^2.0.1",
    "solid-ui": "^1.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/plugin-transform-runtime": "^7.5.5",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/rdflib": "^0.20.1",
    "babel-core": "^6.24.0",
    "babel-loader": "^8.0.2",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^2.1.1",
    "file-loader": "^4.2.0",
    "font-awesome": "^4.7.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.5",
    "lint-staged": "^9.2.5",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "prettier-standard": "^9.1.1",
    "sass-loader": "^7.3.1",
    "typescript": "^3.6.2",
    "url-loader": "^2.0.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-sources": "^1.4.3"
  },
  "standard": {
    "globals": [
      "$rdf",
      "tabulator",
      "QUnit"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/*.ts": [
      "prettier-standard",
      "git add"
    ]
  }
}
